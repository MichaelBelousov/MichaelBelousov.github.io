idth:1280px;--fixed-max-width:30rem;--min-page-height:calc(100vh - 57px - 44px)}@media screen and (max-width:320px){:root{--padding-l:var(--iui-l);--padding-m:var(--text-padding)}}@media screen and (min-width:321px){:root{--padding-l:var(--iui-xl);--padding-m:var(--text-padding)}}#page-header,#page-header-with-breadcrumbs{padding:45px var(--margin) var(--text-padding);background-color:var(--navigation-pane-bg-color);border-bottom:1px solid var(--navigation-pane-border-color);z-index:1}#page-header-with-breadcrumbs .iui-text-headline,#page-header .iui-text-headline{margin:0 0 11px}#page-header-with-breadcrumbs{padding:0 var(--margin) var(--text-padding)}#page-header-with-breadcrumbs .breadcrumbs{margin-left:-12px;margin-bottom:16px}.tutorial-tile{border-radius:3px;width:250px!important;height:100%}.tutorial-tile .iui-thumbnail{border-radius:3px 3px 0 0}.tutorial-tile .iui-name-label{white-space:normal!important}.tutorial-tile .grid-icon{width:20px!important;height:20px!important}.tutorial-tile .thumbnail-container{display:flex;align-items:center;justify-content:center;width:100%;height:100%}.tutorial-tile .thumbnail-container .group-icon{width:64px!important;height:64px!important;transition:transform .4s ease}.tutorial-tile .tutorial-tile-thumbnail-placeholder,.tutorial-tile .tutorial-tile-thumbnail-placeholder-not-clickable{height:64px!important;width:64px!important}.tutorial-tile .tutorial-tile-thumbnail-placeholder-not-clickable path,.tutorial-tile .tutorial-tile-thumbnail-placeholder path{fill:rgba(0,0,0,.4);fill:var(--iui-icons-color)}.tutorial-tile .tutorial-tile-thumbnail-placeholder{transition:transform .4s ease}.tutorial-tile:focus-within .tutorial-tile-thumbnail-placeholder,.tutorial-tile:hover .tutorial-tile-thumbnail-placeholder{transform:translateZ(0) scale(1.2)}